<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shap.shap_office">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.shap.shap_office.ContactUs"></activity>
        <activity android:name="com.shap.shap_office.DetailKategori" />
        <activity
            android:name="com.shap.shap_office.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.shap.shap_office.ui.splashscreen.SplashScreenActivity"
            android:theme="@style/Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>